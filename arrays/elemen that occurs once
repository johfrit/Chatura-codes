import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner (System.in);
      int n=sc.nextInt();
      int arr[]=new int[n];
      for(int i=0;i<n;i++){
        arr[i]=sc.nextInt();
      }
      System.out.println(function(arr));
  }
  static int function(int arr[]){
    int once =0;
    for(int nums:arr){
      once^=nums;
    }
    return once;
  }
}

