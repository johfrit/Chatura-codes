class Solution {
    public int subarraySum(int[] arr, int k) {
        int n=arr.length;
        int prefix=0,count=0;
        HashMap<Integer, Integer>hm=new HashMap<>();
        hm.put(0,1);
        for(int i=0;i<n;i++){
            prefix+=arr[i];
            int remove=prefix-k;
            count+=hm.getOrDefault(remove,0);
            hm.put(prefix,hm.getOrDefault(prefix,0)+1);
        }
        return count;
    }
}
